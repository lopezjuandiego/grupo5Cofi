<!DOCTYPE html>
<html lang="es">
<%- include("../includes/head") %>

    <body>
        <%- include("../includes/header") %>

            <main>
                <section>
                    <% if (users.length > 0) { %>
                        <article ">Hay <%= users.length %> registrados con ese Apellido/s</article>
         
            <% users.forEach(element => { %>
                <article class=" listagral">
                            <a class="contentlista" href="/users/profile/<%= element.id %> ">
                                <% if (users.avatar) { %>
                                    <img class="contentimagen" src="/uploads/avatars/<%= element.avatars.Url %> " alt=""
                                        srcset="">
                                    <% } %>
                                        <ul class="contlis">

                                            <li class="lilista">NOMBRE: <%= element.nombre %>
                                            </li>

                                            <li class="lilista">APELLIDO: <%= element.apellido %>
                                            </li>

                                            <li class="lilista">E-MAIL: <%= element.email %>
                                            </li>


                                        </ul>
                            </a>
                        </article>

                        <% }) %>

                            <% } else { %>
                                <article>No se encontraron usuarios</article>
                                <% }%>

                                    <article class="iconos1">
                                        <a class="boton1" href="/users/index">VOLVER</a>
                                    </article>


                </section>
            </main>
            <%- include("../includes/footer") %>
    </body>

</html>